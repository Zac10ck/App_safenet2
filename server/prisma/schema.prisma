datasource postgres {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     String[]
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model Safenet {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  key       String?
  password  String?
  remarks   Int?
  updatedAt DateTime @updatedAt
  userId    Int?
  username  String?
}